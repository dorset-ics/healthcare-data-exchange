{
    "resourceType": "Bundle",
    "type": "transaction",
    "entry": [
        {
			{% evaluate uniqueId using '_base/Common/ID/Organization' OdsCode: msg.id -%}
			
            "resource": {
                "resourceType": "Organization",
                "id": "{{ uniqueId }}",
				"name": "{{ msg.id }}",
                "meta": {
                    "profile": [
                        "https://fhir.hl7.org.uk/StructureDefinition/UKCore-Organization"
                    ]
                }
            },
			"request":{
				"method":"PUT",
				"url":"Organization/{{ uniqueId }}",
			},
        }
    ]
}